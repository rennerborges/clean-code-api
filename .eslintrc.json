{
    "env": {
        "node": true,
        "commonjs": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-eq-null": "off",
        "@typescript-eslint/strict-boolean-expressions": "off",
        "no-return-await": "off",
        "@typescript-eslint/return-await": "error"
    }
}